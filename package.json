{
  "name": "@assetsfirst/pasarguard-sdk",
  "version": "1.1.0",
  "description": "Fully typed client SDK for the Pasarguard API, supporting both browser and Node.js environments.",
  "author": "pods611",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "rimraf": "rimraf",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "codegen": "openapi-generator-cli generate --generator-key v1 --type-mappings=anyOf=object",
    "test": "openapi-generator-cli author template -g typescript-axios -o ./templates/typescript-axios",
    "openapi-generator-cli": "openapi-generator-cli",
    "format": "prettier --write .",
    "lint": "eslint --fix ."
  },
  "private": false,
  "dependencies": {
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@openapitools/openapi-generator-cli": "^2.24.0",
    "@types/node": "^24.6.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "globals": "^16.4.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "pasarguard-sdk",
    "pasarguard",
    "pasarguard api",
    "pasarguard api client",
    "pasarguard vpn",
    "pasarguard vpn api",
    "pasarguard vpn api client",
    "vpn",
    "http-client",
    "api-client",
    "proxy",
    "shadowsocks",
    "v2ray",
    "xray",
    "trojan",
    "wireguard",
    "vmess",
    "vless",
    "hysteria",
    "tuic",
    "rest-api",
    "http",
    "networking"
  ]
}
